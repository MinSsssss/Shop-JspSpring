<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sian.product.dao.ProductAttachDAO">
	
	<insert id="insert">
		INSERT INTO tbl_attach(uuid,uploadpath,filename,filetype,product_no)
		VALUES(#{uuid},#{uploadPath},#{fileName},#{fileType},#{product_no})
	</insert>
	
	<delete id="delete">
		DELETE FROM tbl_attach WHERE uuid=#{uuid}
	</delete>
	
	<select id="findBiProduct_no" resultType="com.sian.product.dto.ProductAttachDTO">
		SELECT * FROM tbl_attach WHERE product_no=#{product_no}
		
	</select>

	
	
	
</mapper>
